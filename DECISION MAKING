#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
cost=(A*350.34)+(B*230.90)+(C*190.55)+(D*125.30)+(E*180.90)
if cost<15000:
    print("yes")
else:
    print("no")
    
    
#Sece Dining
S=input()
R=int(input())
if S=="front":
  if R==1:
      print("Left Handed")
  elif R==2:
      print("Right Handed")
  else:
      print("Invalid Input")
elif S=="rear":
    if R==1:
        print("Right Handed")
    elif R==2:
        print("Left Handed")
    else:
        print("Invalid Input")


#Intersection of a Circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
x=x1-x2
y=y1-y2
cc=(x**2+y**2)**0.5
if cc==r1+r2:
    print("The circles are tangential")
elif cc<r1+r2:
    print("The circles overlap")
else:
    print("The circles do not overlap")
    
    
#Microwave Oven
a=int(input("Enter the number of items\n"))
b=float(input("Enter the single item heating time\n"))
if a==2:
    print("The recommended heating time is %.2f"%(b+0.5*b))
elif a==3: 
    print("The recommended heating time is %.2f"%(2*b))
else:
    print("Number of items is more")
    
    
