#Arithmetic Operators
def arith(n1,n2):
    print(n1+n2)
    print(n1-n2)
    print(n1*n2)
    print(n1/n2)
    print(n1//n2)
    print(n1%n2)
a=int(input())
b=int(input())
arith(a,b)


#Sqaure Root
def sqrt(n):
    print(n**2)
a=int(input())
sqrt(a)


#Cube Root
def cube(n):
    print(n**3)
a=int(input())
cube(a)
